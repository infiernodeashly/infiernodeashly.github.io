  "devDependencies": {
    "concurrently": "^4.1.0",
    "gh-pages": "^3.2.3",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "deploy": "^1.0.3",
    "express": "^4.17.1",
    "heroku-cli": "^7.0.9",
    "if-env": "^1.0.4"
  }
}